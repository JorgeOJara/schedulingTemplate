{
  "name": "scheduling-template",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/web",
    "apps/api",
    "packages/shared"
  ],
  "scripts": {
    "dev": "concurrently \"bun --filter ./apps/web\" \"bun --filter ./apps/api\"",
    "build": "turbo run build",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "test": "turbo run test"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "turbo": "^2.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.1.0"
  },
  "packageManager": "bun@1.1.38"
}
